# Distribusjon (dcat:Distribution)
:Distribution_Shape
    a sh:NodeShape ;
    rdfs:label "Distribution"@en , "Distribusjon"@nb ;
    sh:property [ # tilgangsURL
        sh:minCount 1 ;
        sh:nodeKind sh:BlankNodeOrIRI;
        sh:path dcat:accessURL ;
        sh:severity sh:Violation
    ], [ # beskrivelse
        sh:nodeKind sh:Literal ;
        sh:path dct:description ;
        sh:severity sh:Violation
    ], [ # format
#        sh:class dct:MediaTypeOrExtent ;
        sh:nodeKind sh:IRI ;
        # sh:maxCount 1 ; # Norwegian extension 0..n
        sh:path dct:format ;
        sh:severity sh:Violation
    ], [ # lisens, sjekker kun klasse her
#        sh:class dct:LicenseDocument ;
        sh:nodeKind sh:IRI ;
#        sh:maxCount 1 ;
        sh:path dct:license ;
        sh:severity sh:Violation ;
        sh:message "Range: Det som dct:license peker på er ikke en instans av dct:LicenseDocument, dvs. en IRI."@nb ;
    ], [ # lisens, sjekker kun maks. antall her
#        sh:class dct:LicenseDocument ;
        sh:nodeKind sh:IRI ;
        sh:maxCount 1 ;
        sh:path dct:license ;
        sh:severity sh:Violation ;
        sh:message "Multiplisitet: dct:license kan ha maks. 1 verdi."@nb ;
    ], [ # lisens, sjekker kun min. antall her, fordi dct:license er anbefalt, warning hvis ikke med
#        sh:class dct:LicenseDocument ;
        sh:minCount 1 ;
        sh:path dct:license ;
        sh:severity sh:Warning ;
        sh:message "Kravsnivå: Fant ingen lisens (dct:license). Lisens er anbefalt egenskap for Distribusjon."@nb ;
    ], [ # status 
#        sh:class skos:Concept ;
        sh:nodeKind sh:IRI ;
        sh:maxCount 1 ;
        sh:path adms:status ;
        sh:severity sh:Violation
    ], [ # tilgjengelighet
#        sh:class skos:Concept ;
        sh:nodeKind sh:IRI ;
        sh:maxCount 1 ;
        sh:path dcatap:availability ;
        sh:severity sh:Violation
    ], [ # dokumentasjon 
#        sh:class foaf:Document ;
        sh:nodeKind sh:IRI ;
        sh:path foaf:page ;
        sh:severity sh:Violation
    ], [ # endringsdato 
        sh:maxCount 1 ;
        sh:node :DateOrDateTimeDataType_Shape ;
        sh:path dct:modified ;
        sh:severity sh:Violation
    ], [ # filstørrelse 
        sh:datatype xsd:decimal ;
        sh:maxCount 1 ;
        sh:path dcat:byteSize ;
        sh:severity sh:Violation
    ], [ # i samsvar med  
        sh:class dct:Standard ;
        sh:path dct:conformsTo ;
        sh:severity sh:Violation
    ], [ # kompremeringsformat 
#        sh:class dct:MediaType ;
        sh:nodeKind sh:IRI ;
        sh:maxCount 1 ;
        sh:path dcat:compressFormat ;
        sh:severity sh:Violation
    ], [ # medietype 
#        sh:class dct:MediaType ;
        sh:nodeKind sh:IRI ;
        sh:path dcat:mediaType ;
        sh:severity sh:Violation
    ], [ # nedlastsingslenke
        sh:nodeKind sh:BlankNodeOrIRI;
        sh:path dcat:downloadURL ;
        sh:severity sh:Violation
    ], [ # pakkeformat 
#        sh:class dct:MediaType ;
        sh:nodeKind sh:IRI ;
        sh:maxCount 1 ;
        sh:path dcat:packageFormat ;
        sh:severity sh:Violation
    ], [ # policy
        sh:class odrl:Policy ;
        sh:maxCount 1 ;
        sh:path odrl:hasPolicy ;
        sh:severity sh:Violation
    ], [ # rettigheter 
        sh:class dct:RightsStatement ;
        sh:maxCount 1 ;
        sh:path dct:rights ;
        sh:severity sh:Violation
    ], [ # romlig oppløsning
        sh:datatype xsd:decimal ;
        sh:path dcat:spatialResolutionInMeters ;
        sh:severity sh:Violation
    ], [ # sjekksum 
        sh:class spdx:Checksum ;
        sh:maxCount 1 ;
        sh:path spdx:checksum ;
        sh:severity sh:Violation
    ], [ # språk
#        sh:class dct:LinguisticSystem ;
        sh:nodeKind sh:IRI ;
        sh:path dct:language ;
        sh:severity sh:Violation
    ], [ # tidsoppløsning 
        sh:datatype xsd:duration ;
        sh:path dcat:temporalResolution ;
        sh:severity sh:Violation
    ], [ # tilgangstjeneste 
        sh:class dcat:DataService ;
        sh:path dcat:accessService ;
        sh:severity sh:Violation
    ], [ # tittel 
        sh:nodeKind sh:Literal ;
        sh:path dct:title ;
        sh:severity sh:Violation
    ], [ # utgivelsesdato 
        sh:maxCount 1 ;
        sh:node :DateOrDateTimeDataType_Shape ;
        sh:path dct:issued ;
        sh:severity sh:Violation
    ] ;
    sh:targetClass dcat:Distribution .

# Dokument (foaf:Document)
