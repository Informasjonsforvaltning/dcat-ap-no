#-------------------------------------------------------------------------
# The shapes in this file cover all classes in DCAT-AP-NO 2.2.x.
#-------------------------------------------------------------------------

# Aktør (foaf:Agent)
:Agent_Shape
    a sh:NodeShape ;
    rdfs:label "Agent"@en , "Aktør"@nb ;
    sh:property [ # navn
        sh:minCount 1 ;
        sh:nodeKind sh:Literal ;
        sh:path foaf:name ;
        sh:severity sh:Violation
    ], [ # organisasjonsidentifikator
        sh:maxCount 1 ;
        sh:nodeKind sh:Literal ;
        sh:path dct:identifier ;
        sh:severity sh:Violation
    ], [ # utgivertype
#        sh:class skos:Concept ;
        sh:nodeKind sh:IRI ;
        sh:maxCount 1 ;
        sh:path dct:type ;
        sh:severity sh:Violation
    ] ;
    sh:and (
      sh:or (
        [sh:targetObjectsOf dct:publisher]
        [sh:targetObjectsOf dct:creator ] )
      sh:or (
        [sh:targetSubjectOf dcat:Dataset]
        [sh:targetSubjectOf dcat:Catalog]
        [sh:targetSUbjectOf dcat:DataService] )
      ) ;
    # sh:targetClass foaf:Agent ;
   .

# Datasett (dcat:Dataset)
:Dataset_Shape
    a sh:NodeShape ;
    rdfs:label "Dataset"@en , "Datasett"@nb ;
    sh:property [ # beskrivelse
        sh:minCount 1 ;
        sh:nodeKind sh:Literal ;
        sh:path dct:description ;
        sh:severity sh:Violation
    ], [ # identifikator
        sh:minCount 1 ;
        sh:nodeKind sh:Literal ;
        sh:path dct:identifier ;
        sh:severity sh:Violation
    ], [ # tema
#        sh:class skos:Concept ;
        sh:nodeKind sh:IRI ;
        sh:minCount 1 ; # Norwegian extension: Added minCount 1
        sh:path dcat:theme ;
        sh:severity sh:Violation
    ], [ # tittel
        sh:minCount 1 ;
        sh:nodeKind sh:Literal ;
        sh:path dct:title ;
        sh:severity sh:Violation
    ], [ # utgiver
#        sh:class foaf:Agent ;
        sh:nodeKind sh:IRI ;
        sh:maxCount 1 ;
        sh:minCount 1 ; # Norwegian extension: Added minCount 1
        sh:path dct:publisher ;
        sh:severity sh:Violation
    ], [ # begrep, Norwegian extension: Added this property
        sh:class skos:Concept ;
        sh:path dct:subject ;
        sh:severity sh:Violation
    ], [ # ble generert ved 
        sh:class prov:Activity ;
        sh:path prov:wasGeneratedBy ;
        sh:severity sh:Violation
    ], [ # datasettdistribusjon
        sh:class dcat:Distribution ;
        sh:path dcat:distribution ;
        sh:severity sh:Violation
    ], [ # dekningsområde
#        sh:class dct:Location ;
        sh:nodeKind sh:IRI ;
        sh:path dct:spatial ;
        sh:severity sh:Violation
    ], [ # emneord
        sh:nodeKind sh:Literal ;
        sh:path dcat:keyword ;
        sh:severity sh:Violation
    ], [ # følger
        sh:class cpsv:Rule ;
        sh:path cpsv:follows ;
        sh:severity sh:Violation
    ], [ # kontaktpunkt
        sh:or (
            [ sh:class vcard:Kind ]
            [ sh:class vcard:Organization ] ) ;
        sh:path dcat:contactPoint ;
        sh:severity sh:Violation
    ], [ # tidsrom
	    sh:class dct:PeriodOfTime ;
        sh:path dct:temporal ;
        sh:severity sh:Violation
    ], [ # tilgangsnivå
#        sh:class dct:RightsStatement ;
        sh:nodeKind sh:IRI ;
        sh:maxCount 1 ;
        sh:path dct:accessRights ;
        sh:severity sh:Violation
    ], [ # annen identifikator
        sh:class adms:Identifier ;
        sh:path adms:identifier ;
        sh:severity sh:Violation
    ], [ # dokumentasjon
#        sh:class foaf:Document ;
        sh:nodeKind sh:IRI ;
        sh:path foaf:page ;
        sh:severity sh:Violation
    ], [ # eksempeldata 
        sh:class dcat:Distribution ;
        sh:path adms:sample ;
        sh:severity sh:Violation
    ], [ # endringsdato
        sh:maxCount 1 ;
        sh:path dct:modified ;
        sh:severity sh:Violation ;
        sh:node :DateOrDateTimeDataType_Shape
    ], [ # er del av
        sh:class dcat:Dataset ;
        sh:path dct:isPartOf ;
        sh:severity sh:Violation
    ], [ # er påkrevd av 
        sh:class dcat:Dataset ;
        sh:path dct:isRequiredBy ;
        sh:severity sh:Violation
    ], [ # er referert av
        sh:nodeKind sh:BlankNodeOrIRI ;
        sh:path dct:isReferencedBy ;
        sh:severity sh:Violation
    ], [ # er versjon av
        sh:class dcat:Dataset ;
        sh:path dct:isVersionOf ;
        sh:severity sh:Violation
    ], [ # erstatter 
        sh:class dcat:Dataset ;
        sh:path dct:replaces ;
        sh:severity sh:Violation
    ], [ # erstattes av
        sh:class dcat:Dataset ;
        sh:path dct:isReplacedBy ;
        sh:severity sh:Violation
    ], [ # forrige 
        sh:maxCount 1 ;
        sh:class dcat:Dataset ;
        sh:path dcat:prev ;
        sh:severity sh:Violation
    ], [ # frekvens
#        sh:class dct:Frequency ;
        sh:nodeKind sh:IRI ;
        sh:maxCount 1 ;
        sh:path dct:accrualPeriodicity ;
        sh:severity sh:Violation
    ], [ # har del
        sh:class dcat:Dataset ;
        sh:path dct:hasPart ;
        sh:severity sh:Violation
    ], [ # har kvalitetsnote
        sh:class dqv:QualityAnnotation ;
        sh:path dqv:hasQualityAnnotation ;
        sh:severity sh:Violation
    ], [ # har måleresultat
        sh:class dqv:QualityMeasurement ;
        sh:path dqv:hasQualityMeasurement ;
        sh:severity sh:Violation
    ], [ # har versjon
        sh:class dcat:Dataset ;
        sh:path dct:hasVersion ;
        sh:severity sh:Violation
    ], [ # i samsvar med
        sh:class dct:Standard ;
        sh:path dct:conformsTo ;
        sh:severity sh:Violation
    ], [ # i serie
        sh:class dcat:DatasetSeries ;
        sh:path dcat:inSeries ;
        sh:severity sh:Violation
    ], [ # kilde 
        sh:class dcat:Dataset ;
        sh:path dct:source ;
        sh:severity sh:Violation
    ], [ # krever 
        sh:class dcat:Dataset ;
        sh:path dct:requires ;
        sh:severity sh:Violation
    ], [ # kvalifisert kreditering
        sh:class prov:Attribution ;
        sh:path prov:qualifiedAttribution ;
        sh:severity sh:Violation
    ], [ # kvalifisert relasjon
        sh:class dcat:Relationship ;
        sh:path dcat:qualifiedRelation ;
        sh:severity sh:Violation
    ], [ # landingsside
#        sh:class foaf:Document ;
        sh:nodeKind sh:IRI ;
        sh:path dcat:landingPage ;
        sh:severity sh:Violation
    ], [ # opphav
        sh:class dct:ProvenanceStatement ;
        sh:path dct:provenance ;
        sh:severity sh:Violation
    ], [ # produsent
#        sh:class foaf:Agent ;
        sh:nodeKind sh:IRI ;
        sh:maxCount 1 ;
        sh:path dct:creator ;
        sh:severity sh:Violation
    ], [ # refererer til 
        sh:nodeKind sh:BlankNodeOrIRI ;
        sh:path dct:references ;
        sh:severity sh:Violation
    ], [ # relatert ressurs 
        sh:nodeKind sh:BlankNodeOrIRI ;
        sh:path dct:relation ;
        sh:severity sh:Violation
    ], [ # romlig oppløsning
        sh:datatype xsd:decimal ;
        sh:path dcat:spatialResolutionInMeters ;
        sh:severity sh:Violation
    ], [ # språk
#        sh:class dct:LinguisticSystem ;
        sh:nodeKind sh:IRI ;
        sh:path dct:language ;
        sh:severity sh:Violation
    ], [ # tidsoppløsning
        sh:datatype xsd:duration ;
        sh:path dcat:temporalResolution ;
        sh:severity sh:Violation
    ], [ # type
#        sh:class skos:Concept ;
        sh:nodeKind sh:IRI ;
        sh:maxCount 1 ; 
        sh:path dct:type ;
        sh:severity sh:Violation
    ], [ # utgivelsesdato
        sh:maxCount 1 ;
        sh:path dct:issued ;
        sh:severity sh:Violation ;
        sh:node :DateOrDateTimeDataType_Shape
    ], [ # versjon 
        sh:maxCount 1 ;
        sh:nodeKind sh:Literal ;
        sh:path owl:versionInfo ;
        sh:severity sh:Violation
    ], [ # versjonsnote 
        sh:nodeKind sh:Literal ;
        sh:path adms:versionNotes ;
        sh:severity sh:Violation
    ] ;
    sh:targetClass dcat:Dataset .

# Datasett serie (dcat:DatasetSeries)
:DatasetSerie_Shape
    a sh:NodeShape ;
    rdfs:label "Dataset series"@en , "Datasett serie"@nb ;
    sh:property [ # første
        sh:minCount 1 ;
        sh:maxCount 1 ;
        sh:class dcat:Dataset ; 
        sh:path dcat:first ;
        sh:severity sh:Violation ;
    ], [ # siste 
        sh:maxCount 1 ;
        sh:class dcat:Dataset ; 
        sh:path dcat:last ;
        sh:severity sh:Violation ;
    ] ;
    sh:targetClass dcat:DatasetSeries .

# Datatjeneste (dcat:DataService)
