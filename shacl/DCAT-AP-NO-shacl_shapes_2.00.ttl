    a sh:NodeShape ;
    sh:property [
        sh:node :AccessRightsRestriction ;
        sh:nodeKind sh:IRI ;
        sh:path dct:accessRights ;
        sh:severity sh:Violation
    ], [
        sh:node :FileTypeRestriction ;
