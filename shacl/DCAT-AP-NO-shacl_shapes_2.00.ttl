    a sh:NodeShape ;
    rdfs:label "Data Service"@en , "Datatjeneste"@nb ;
    sh:property [ # endepunktURL
        sh:maxCount 1 ; # Norwegian extension: added maxCount 1
        sh:minCount 1 ;
        sh:nodeKind sh:BlankNodeOrIRI ;
        sh:path dcat:endpointURL ;
        sh:severity sh:Violation
    ], [ # identifikator
        sh:minCount 1 ;
        sh:nodeKind sh:Literal ;
        sh:path dct:identifier ;
        sh:severity sh:Violation
    ], [ # tittel
        sh:minCount 1 ;
        sh:nodeKind sh:Literal ;
        sh:path dct:title ;
        sh:severity sh:Violation
    ], [ # emneord
        sh:nodeKind sh:Literal ;
        sh:path dcat:keyword ; 
        sh:severity sh:Violation
    ], [ # endepunktsbeskrivelse
        sh:nodeKind sh:BlankNodeOrIRI ;
        sh:path dcat:endpointDescription ;
        sh:severity sh:Violation
    ], [ # format, Norwegian extension: Added dct:format
#        sh:class dct:MediaTypeOrExtent ;
        sh:nodeKind sh:IRI ;
        sh:path dct:format ;
        sh:severity sh:Violation
    ], [ ## format, Norwegian extension, check Controlled Vocab for dct:format
        sh:node :FileTypeRestriction ;
        sh:nodeKind sh:IRI ;
        sh:path dct:format ;
        sh:severity sh:Violation
     ], [ # kontaktpunkt 
        sh:or (
            [ sh:class vcard:Kind ]
            [ sh:class vcard:Organization ] ) ;
        sh:path dcat:contactPoint ;
        sh:severity sh:Violation 
     ], [ # tema 
#        sh:class skos:Concept ;
        sh:nodeKind sh:IRI ;
        sh:path dcat:theme ;
        sh:severity sh:Violation 
     ], [ # tilgjengeliggjør datasett
        sh:class dcat:Dataset ;
        sh:path dcat:servesDataset ;
        sh:severity sh:Violation
    ], [ # utgiver
        sh:minCount 1 ;
#        sh:class foaf:Agent ;
        sh:nodeKind sh:IRI ;
        sh:path dct:publisher ;
        sh:severity sh:Violation
    ], [ # beskrivelse
        sh:nodeKind sh:Literal ;
        sh:path dct:description ;
        sh:severity sh:Violation
    ], [ # dokumentasjon
#        sh:class foaf:Document ;
        sh:nodeKind sh:IRI ;
        sh:path foaf:page ;
        sh:severity sh:Violation
    ], [ # følger
        sh:class cpsv:Rule ;
        sh:path cpsv:follows ;
        sh:severity sh:Violation
    ], [ # i samsvar med 
        sh:class dct:Standard ;
        sh:path dct:conformsTo ;
        sh:severity sh:Violation
    ], [ # landingsside  
        sh:maxCount 1 ;
#        sh:class foaf:Document ;
        sh:nodeKind sh:IRI ;
        sh:path dcat:landingPage ;
        sh:severity sh:Violation
    ], [ # lisens 
#        sh:class dct:LicenseDocument ;
        sh:nodeKind sh:IRI ;
        sh:maxCount 1 ;
        sh:path dct:license ;
        sh:severity sh:Violation
   ], [ # medietype, Norwegian extension: Added dcat:mediaType
#        sh:class dct:MediaType ;
        sh:nodeKind sh:IRI ;
        sh:path dcat:mediaType ;
        sh:severity sh:Violation
    ], [ # tilgangsrettigheter
#        sh:class dct:RightsStatement ;
        sh:nodeKind sh:IRI ;
        sh:maxCount 1 ;
        sh:path dct:accessRights ;
        sh:severity sh:Violation
    ], [ # type
        sh:maxCount 1 ;
        sh:path dct:type ;
